<UserControl x:Class="TogglDesktop.WPF.AutoComplete.GrayTextCategory"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:toggl="clr-namespace:TogglDesktop.WPF"
             mc:Ignorable="d" d:DesignWidth="300"
             Style="{toggl:Resources ResourceKey=AutoCompleteItemBase}"
             Padding="8, 0, 0, 0"
             >
    <TextBlock Name="categoryName" x:FieldModifier="private"
               Foreground="#a1a1a1"
               FontSize="11"
               Margin="0, 5, 0, 2"
               >This is a category</TextBlock>
</UserControl>
